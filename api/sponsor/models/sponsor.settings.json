{
  "kind": "collectionType",
  "collectionName": "sponsors",
  "info": {
    "name": "Sponsor"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "url": {
      "type": "string",
      "required": true
    },
    "logo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    }
  }
}
